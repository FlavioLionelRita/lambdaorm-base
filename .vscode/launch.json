	"version": "0.2.0",
	"configurations": [
		{
			"name": "introspect",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/dev/introspect/index.ts"
		},
		{
			"name": "match",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/dev/match/index.ts",
			"envFile": "${workspaceFolder}/src/dev/match/.env"
		},
		{
			"name": "complete",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/dev/complete/index.ts",
			"envFile": "${workspaceFolder}/src/dev/complete/.env"
		}			
	]
}